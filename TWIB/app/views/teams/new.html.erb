<h1>New Team</h1>


<%= form_tag(teams_url, method: 'post') do %>


  <div>
    <%= label_tag :team_name %><br />
    <%= text_field_tag :team_name %>
  </div>

  <div>
    <%= label_tag :conference %><br />
    <%= text_field_tag :conference %>
  </div>

  <div>
    <%= label_tag :division %><br />
    <%= text_field_tag :division %>
  </div>


  <div>
    <%= submit_tag "Create Team" %>
  </div>
<% end %>
